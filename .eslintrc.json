{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "indent": ["error", 2],
      "linebreak-style": ["error", "windows"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
    }
  }